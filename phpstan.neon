parameters:

    paths:
        - app/

    # Level 9 is the highest level
    level: 0

    ignoreErrors:
        - '#PHPDoc tag @var#'
        - '#Call to an undefined method Illuminate\\Support\\HigherOrder#'

        
    excludePaths:
        - ./app/Actions/*
#
#    checkMissingIterableValueType: false

    checkGenericClassInNonGenericObjectType: false
